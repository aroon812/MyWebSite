<h1>Add new photo</h1>

<%= form_for(Photo.new, url: photo_create_url, :html => { :multipart => true }) do |f| %>
<p><label for = "photo_project_id">ProjectID</label>:

<%= f.text_field :project_id, value: @project_id%></p>
<p><label for = "photo_description">Description</label>:

<%= f.text_area :description %></p>

<%= f.file_field :file %>

<%= submit_tag "Create" %>
<% end -%>
<%= link_to 'Back', {:action => 'show', :controller => 'project', :id => @project_id} %>